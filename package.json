{
  "name": "pollypocket",
  "version": "1.0.0",
  "description": "capstone project",
  "engines": {
    "node": "10.x"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "script/deploy",
    "heroku-token": "script/encrypt-heroku-auth-token",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "postinstall": "touch secrets.js",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teenage-soup-for-the-chicken-soul/pollypocket.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teenage-soup-for-the-chicken-soul/pollypocket.git"
  },
  "homepage": "https://github.com/teenage-soup-for-the-chicken-soul/pollypocket.git",
  "dependencies": {
    "@cloudant/cloudant": "^4.1.1",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.1.2",
    "axios": "^0.19.0",
    "compression": "^1.7.4",
    "connect-session-sequelize": "^6.0.0",
    "express": "^4.17.1",
    "express-session": "^1.15.1",
    "history": "^4.9.0",
    "html-react-parser": "^0.8.0",
    "jsdom": "^16.5.0",
    "material-design-icons": "^3.0.1",
    "morgan": "^1.9.1",
    "nano": "^8.1.0",
    "nodemon": "^1.19.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "pouchdb": "^7.1.1",
    "pouchdb-find": "^7.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-vis": "^1.11.7",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
